ame":"token","doc":null,"default_value":"","external_name":"token","restriction":"String"}],"args_string":"(token : String)","source_link":"https://github.com/Xosmond/telegrambot.cr/blob/2cb845e78928ae004c7bcede471cd4fd1f89dcc4/src/telegrambot/api.cr#L18","source_link":"https://github.com/Xosmond/telegrambot.cr/blob/2cb845e78928ae004c7bcede471cd4fd1f89dcc4/src/telegrambot/api.cr#L18","def":{"name":"new","args":[{"name":"token","doc":null,"default_value":"","external_name":"token","restriction":"String"}],"double_splat":null,"splat_index":null,"yields":null,"block_arg":null,"return_type":"","visibility":"Public","body":"_ = allocate\n_.initialize(token)\nif _.responds_to?(:finalize)\n  ::GC.add_finalizer(_)\nend\n_\n"}}],"instance_methods":[{"id":"client=(client:HTTP::Client)-instance-method","html_id":"client=(client:HTTP::Client)-instance-method","name":"client=","doc":null,"summary":null,"abstract":false,"args":[{"name":"client","doc":null,"default_value":"","external_name":"client","restriction":"HTTP::Client"}],"args_string":"(client : HTTP::Client)","source_link":"https://github.com/Xosmond/telegrambot.cr/blob/2cb845e78928ae004c7bcede471cd4fd1f89dcc4/src/telegrambot/api.cr#L7","source_link":"https://github.com/Xosmond/telegrambot.cr/blob/2cb845e78928ae004c7bcede471cd4fd1f89dcc4/src/telegrambot/api.cr#L7","def":{"name":"client=","args":[{"name":"client","doc":null,"default_value":"","external_name":"client","restriction":"HTTP::Client"}],"double_splat":null,"splat_index":null,"yields":null,"block_arg":null,"return_type":"","visibility":"Public","body":"@client = client"}},{"id":"get_updates(offset:Int)-instance-method","html_id":"get_updates(offset:Int)-instance-method","name":"get_updates","doc":"Get updates from the bot sice the last *offset*\n\nGet all updates\n```\napi.get_updates 0 # => 13171, [Messages]\n```\nGet since offset\n```\napi.get_updates offset: 1231796 # => 1231886, [Messages]\n```","summary":"<p>Get updates from the bot sice the last <em>offset</em></p>","abstract":false,"args":[{"name":"offset","doc":null,"default_value":"","external_name":"offset","restriction":"Int"}],"args_string":"(offset : Int)","source_link":"https://github.com/Xosmond/telegrambot.cr/blob/2cb845e78928ae004c7bcede471cd4fd1f89dcc4/src/telegrambot/api.cr#L50","source_link":"https://github.com/Xosmond/telegrambot.cr/blob/2cb845e78928ae004c7bcede471cd4fd1f89dcc4/src/telegrambot/api.cr#L50","def":{"name":"get_updates","args":[{"name":"offset","doc":null,"default_value":"","external_name":"offset","restriction":"Int"}],"double_splat":null,"splat_index":null,"yields":null,"block_arg":null,"return_type":"","visibility":"Public","body":"json = call(get_path(\"/getUpdates\"), {offset: offset.to_s})\nif json[\"ok\"].as_bool\n  json_messages = json[\"result\"]\n  messages = [] of Telegrambot::Message\n  json_messages.each do |json_message|\n    messages << (Telegrambot::Message.new(json_message[\"message\"]))\n    offset = json_message[\"update_id\"].as_i\n  end\n  if json_messages.size > 0\n    return {(    offset + 1), messages}\n  else\n    return {offset, messages}\n  end\nelse\n  raise(Telegrambot::Exceptions::ResponseError.new((  \"Couldnt get updates\")))\nend\n"}},{"id":"headers=(headers:HTTP::Headers)-instance-method","html_id":"headers=(headers:HTTP::Headers)-instance-method","name":"headers=","doc":null,"summary":null,"abstract":false,"args":[{"name":"headers","doc":null,"default_value":"","external_name":"headers","restriction":"HTTP::Headers"}],"args_string":"(headers : HTTP::Headers)","source_link":"https://github.com/Xosmond/telegrambot.cr/blob/2cb845e78928ae004c7bcede471cd4fd1f89dcc4/src/telegrambot/api.cr#L6","source_link":"https://github.com/Xosmond/telegrambot.cr/blob/2cb845e78928ae004c7bcede471cd4fd1f89dcc4/src/telegrambot/api.cr#L6","def":{"name":"headers=","args":[{"name":"headers","doc":null,"default_value":"","external_name":"headers","restriction":"HTTP::Headers"}],"double_splat":null,"splat_index":null,"yields":null,"block_arg":null,"return_type":"","visibility":"Public","body":"@headers = headers"}},{"id":"listen(offset:Int,&block)-instance-method","html_id":"listen(offset:Int,&amp;block)-instance-method","name":"listen","doc":"Listen for updates and use the block on each message\n\n```\napi.listen(0) do |message|\n  puts message.text # => Hello\nend\n```","summary":"<p>Listen for updates and use the block on each message</p>","abstract":false,"args":[{"name":"offset","doc":null,"default_value":"","external_name":"offset","restriction":"Int"}],"args_string":"(offset : Int, &block)","source_link":"https://github.com/Xosmond/telegrambot.cr/blob/2cb845e78928ae004c7bcede471cd4fd1f89dcc4/src/telegrambot/api.cr#L76","source_link":"https://github.com/Xosmond/telegrambot.cr/blob/2cb845e78928ae004c7bcede471cd4fd1f89dcc4/src/telegrambot/api.cr#L76","def":{"name":"listen","args":[{"name":"offset","doc":null,"default_value":"","external_name":"offset","restriction":"Int"}],"double_splat":null,"splat_index":null,"yields":1,"block_arg":null,"return_type":"","visibility":"Public","body":"offset || (offset = 0)\nwhile true\n  offset, messages = get_updates(offset: offset)\n  messages.each do |message|\n    yield message\n  end\nend\n"}},{"id":"send_message(chat_id:Int,message:String)-instance-method","html_id":"send_message(chat_id:Int,message:String)-instance-method","name":"send_message","doc":"Sends a *message* to the chat with the *chat_id* specified\n\n```\napi.send_message chat_id: 1231241, message: \"Hello\" # => true\n```","summary":"<p>Sends a <em>message</em> to the chat with the <em>chat_id</em> specified</p>","abstract":false,"args":[{"name":"chat_id","doc":null,"default_value":"","external_name":"chat_id","restriction":"Int"},{"name":"message","doc":null,"default_value":"","external_name":"message","restriction":"String"}],"args_string":"(chat_id : Int, message : String)","source_link":"https://github.com/Xosmond/telegrambot.cr/blob/2cb845e78928ae004c7bcede471cd4fd1f89dcc4/src/telegrambot/api.cr#L31","source_link":"https://github.com/Xosmond/telegrambot.cr/blob/2cb845e78928ae004c7bcede471cd4fd1f89dcc4/src/telegrambot/api.cr#L31","def":{"name":"send_message","args":[{"name":"chat_id","doc":null,"default_value":"","external_name":"chat_id","restriction":"Int"},{"name":"message","doc":null,"default_value":"","external_name":"message","restriction":"String"}],"double_splat":null,"splat_index":null,"yields":null,"block_arg":null,"return_type":"","visibility":"Public","body":"json = call(get_path(\"/sendMessage\"), {chat_id: chat_id.to_s, text: message})\nif json[\"ok\"].as_bool\n  true\nelse\n  raise(Telegrambot::Exceptions::ResponseError.new((  \"Couldnt send the message\")))\nend\n"}}],"macros":[],"types":[]},{"html_id":"github.com/Xosmond/telegrambot.cr/Telegrambot/ChatMessage","path":"Telegrambot/ChatMessage.html","kind":"class","full_name":"Telegrambot::ChatMessage","name":"ChatMessage","abstract":false,"superclass":{"html_id":"github.com/Xosmond/telegrambot.cr/Reference","kind":"class","full_name":"Reference","name":"Reference"},"ancestors":[{"html_id":"github.com/Xosmond/telegrambot.cr/Reference","kind":"class","full_name":"Reference","name":"Reference"},{"html_id":"github.com/Xosmond/telegrambot.cr/Object","kind":"class","full_name":"Object","name":"Object"}],"locations":[{"filename":"telegrambot/chat_message.cr","line_number":2,"url":"https://github.com/Xosmond/telegrambot.cr/blob/2cb845e78928ae004c7bcede471cd4fd1f89dcc4/src/telegrambot/chat_message.cr"}],"repository_name":"github.com/Xosmond/telegrambot.cr","program":false,"enum":false,"alias":false,"aliased":"","const":false,"constants":[],"included_modules":[],"extended_modules":[],"subclasses":[],"including_types":[],"namespace":{"html_id":"github.com/Xosmond/telegrambot.cr/Telegrambot","kind":"module","full_name":"Telegrambot","name":"Telegrambot"},"doc":null,"summary":null,"class_methods":[],"constructors":[{"id":"new(json:JSON::Any)-class-method","html_id":"new(json:JSON::Any)-clas